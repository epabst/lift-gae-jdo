<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <!-- TODO Substitute your Google App Engine app ID below to deploy. -->
  <application>your_app_id_goes_here</application>
  <version>1</version>
  <system-properties>
    <property name="in.gae.j" value="true" />
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    <property name="appengine.orm.disable.duplicate.emf.exception" value="false"/>
  </system-properties>
  <sessions-enabled>true</sessions-enabled>
  <static-files>
    <include path="/css/**"/>
    <include path="/images/**"/>
    <include path="/scripts/**"/>
    <exclude path="/**" />
  </static-files>
</appengine-web-app>
